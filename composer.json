{
    "name": "schoolpalm/module-bridge",
    "description": "A bridge library for SchoolPalm and Module-SDK to manage modules.",
    "type": "library",
    "license": "MIT",
    "version": "1.3.2", 
    "authors": [
        {
            "name": "Hassan Mugabo",
            "email": "cybarox@gmail.com"
            
        }
    ],
    "require": {
        "php": "^8.1"
       
    },
    "autoload": {
        "psr-4": {
            "SchoolPalm\\ModuleBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SchoolPalm\\ModuleBridge\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
